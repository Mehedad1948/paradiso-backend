{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true,
    "assets": [{
    "include": "./mail/templates",
    "outDir": "dist/",
    "watchAssets": true
  }]
  }
  
}
